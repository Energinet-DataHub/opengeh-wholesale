{
  "openapi": "3.0.1",
  "info": {
    "title": "Wholesale Web API",
    "version": "3.0"
  },
  "paths": {
    "/v3/calculations": {
      "post": {
        "tags": [
          "Calculation"
        ],
        "summary": "Create a calculation.",
        "operationId": "CreateCalculation",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/CalculationRequestDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/CalculationRequestDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/CalculationRequestDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "format": "uuid"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Calculation"
        ],
        "summary": "Get calculations that matches the criteria specified",
        "operationId": "SearchCalculations",
        "parameters": [
          {
            "name": "gridAreaCodes",
            "in": "query",
            "description": "",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "executionState",
            "in": "query",
            "description": "",
            "schema": {
              "$ref": "#/components/schemas/CalculationState"
            }
          },
          {
            "name": "minExecutionTime",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "maxExecutionTime",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "periodStart",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "periodEnd",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CalculationDto"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v3/calculations/{calculationId}": {
      "get": {
        "tags": [
          "Calculation"
        ],
        "summary": "Returns a calculation matching calculationId.",
        "operationId": "GetCalculation",
        "parameters": [
          {
            "name": "calculationId",
            "in": "path",
            "required": true,
            "description": "CalculationId",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CalculationDto"
                }
              }
            }
          }
        }
      }
    },
    "/v3/SettlementReport/Download": {
      "get": {
        "tags": [
          "SettlementReport"
        ],
        "summary": "Downloads a compressed settlement report for the specified parameters.",
        "operationId": "Download",
        "parameters": [
          {
            "name": "gridAreaCodes",
            "in": "query",
            "required": true,
            "description": "A list of grid areas to create the settlement report for.",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "calculationType",
            "in": "query",
            "required": true,
            "description": "Currently expects BalanceFixing only.",
            "schema": {
              "$ref": "#/components/schemas/CalculationType"
            }
          },
          {
            "name": "periodStart",
            "in": "query",
            "required": true,
            "description": "The start date and time of the period covered by the settlement report.",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "periodEnd",
            "in": "query",
            "required": true,
            "description": "The end date and time of the period covered by the settlement report.",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "energySupplier",
            "in": "query",
            "description": "Optional GLN/EIC identifier for an energy supplier.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "csvFormatLocale",
            "in": "query",
            "description": "Optional locale used to format the CSV file, e.g. da-DK. Defaults to en-US.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": null,
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CalculationDto": {
        "type": "object",
        "description": "An immutable calculation.",
        "additionalProperties": false,
        "properties": {
          "runId": {
            "type": "integer",
            "format": "int64",
            "nullable": true
          },
          "calculationId": {
            "type": "string",
            "format": "uuid"
          },
          "periodStart": {
            "type": "string",
            "format": "date-time"
          },
          "periodEnd": {
            "type": "string",
            "format": "date-time"
          },
          "resolution": {
            "type": "string"
          },
          "unit": {
            "type": "string"
          },
          "executionTimeStart": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "executionTimeEnd": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "executionState": {
            "$ref": "#/components/schemas/CalculationState"
          },
          "areSettlementReportsCreated": {
            "type": "boolean"
          },
          "gridAreaCodes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "calculationType": {
            "$ref": "#/components/schemas/CalculationType"
          },
          "createdByUserId": {
            "type": "string",
            "format": "uuid"
          }
        }
      },
      "CalculationRequestDto": {
        "type": "object",
        "description": "An immutable request to create a calculation.",
        "additionalProperties": false,
        "properties": {
          "calculationType": {
            "$ref": "#/components/schemas/CalculationType"
          },
          "gridAreaCodes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "startDate": {
            "type": "string",
            "format": "date-time"
          },
          "endDate": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "CalculationState": {
        "type": "string",
        "enum": [
          "Pending",
          "Executing",
          "Completed",
          "Failed"
        ]
      },
      "CalculationType": {
        "type": "string",
        "description": "Defines the wholesale calculation type",
        "enum": [
          "BalanceFixing",
          "Aggregation",
          "WholesaleFixing",
          "FirstCorrectionSettlement",
          "SecondCorrectionSettlement",
          "ThirdCorrectionSettlement"
        ]
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "JWT Authorization header using the Bearer scheme. Example: \"Authorization: Bearer {token}\"",
        "scheme": "bearer"
      }
    }
  },
  "security": [
    {
      "Bearer": [
        "Bearer"
      ]
    }
  ]
}