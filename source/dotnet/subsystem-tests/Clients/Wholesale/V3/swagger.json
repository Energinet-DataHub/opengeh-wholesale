{
  "openapi": "3.0.1",
  "info": {
    "title": "Wholesale Web API",
    "version": "3.0"
  },
  "paths": {
    "/v3/batches": {
      "post": {
        "tags": [
          "Batch"
        ],
        "summary": "Create a batch.",
        "operationId": "CreateBatch",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BatchRequestDto"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/BatchRequestDto"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/BatchRequestDto"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "string",
                  "format": "uuid"
                }
              }
            }
          }
        }
      },
      "get": {
        "tags": [
          "Batch"
        ],
        "summary": "Get batches that matches the criteria specified",
        "operationId": "SearchBatches",
        "parameters": [
          {
            "name": "gridAreaCodes",
            "in": "query",
            "description": "",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "executionState",
            "in": "query",
            "description": "",
            "schema": {
              "$ref": "#/components/schemas/BatchState"
            }
          },
          {
            "name": "minExecutionTime",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "maxExecutionTime",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "periodStart",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "periodEnd",
            "in": "query",
            "description": "",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/BatchDto"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/v3/batches/{batchId}": {
      "get": {
        "tags": [
          "Batch"
        ],
        "summary": "Returns a batch matching batchId.",
        "operationId": "GetBatch",
        "parameters": [
          {
            "name": "batchId",
            "in": "path",
            "required": true,
            "description": "BatchId",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Success",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/BatchDto"
                }
              }
            }
          }
        }
      }
    },
    "/v3/SettlementReport/Download": {
      "get": {
        "tags": [
          "SettlementReport"
        ],
        "summary": "Downloads a compressed settlement report for the specified parameters.",
        "operationId": "Download",
        "parameters": [
          {
            "name": "gridAreaCodes",
            "in": "query",
            "required": true,
            "description": "A list of grid areas to create the settlement report for.",
            "schema": {
              "type": "array",
              "items": {
                "type": "string"
              }
            }
          },
          {
            "name": "processType",
            "in": "query",
            "required": true,
            "description": "Currently expects BalanceFixing only.",
            "schema": {
              "$ref": "#/components/schemas/ProcessType"
            }
          },
          {
            "name": "periodStart",
            "in": "query",
            "required": true,
            "description": "The start date and time of the period covered by the settlement report.",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "periodEnd",
            "in": "query",
            "required": true,
            "description": "The end date and time of the period covered by the settlement report.",
            "schema": {
              "type": "string",
              "format": "date-time"
            }
          },
          {
            "name": "energySupplier",
            "in": "query",
            "description": "Optional GLN/EIC identifier for an energy supplier.",
            "schema": {
              "type": "string"
            }
          },
          {
            "name": "csvFormatLocale",
            "in": "query",
            "description": "Optional locale used to format the CSV file, e.g. da-DK. Defaults to en-US.",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": null,
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    },
    "/v3/SettlementReport": {
      "get": {
        "tags": [
          "SettlementReport"
        ],
        "summary": "Returns a stream containing the settlement report for batch with batchId and gridAreaCode.",
        "operationId": "GetSettlementReportAsStreamAsync",
        "parameters": [
          {
            "name": "batchId",
            "in": "query",
            "required": true,
            "description": "BatchId",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          },
          {
            "name": "gridAreaCode",
            "in": "query",
            "required": true,
            "description": "GridAreaCode",
            "schema": {
              "type": "string"
            }
          }
        ],
        "responses": {
          "200": {
            "description": null,
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    },
    "/v3/SettlementReport/ZippedBasisDataStream": {
      "get": {
        "tags": [
          "SettlementReport"
        ],
        "summary": "Returns a stream containing the settlement report for a batch matching batchId",
        "operationId": "ZippedBasisDataStream",
        "parameters": [
          {
            "name": "batchId",
            "in": "query",
            "required": true,
            "description": "BatchId",
            "schema": {
              "type": "string",
              "format": "uuid"
            }
          }
        ],
        "responses": {
          "200": {
            "description": null,
            "content": {
              "application/octet-stream": {
                "schema": {
                  "type": "string",
                  "format": "binary"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "BatchDto": {
        "type": "object",
        "description": "An immutable batch.",
        "additionalProperties": false,
        "properties": {
          "runId": {
            "type": "integer",
            "format": "int64",
            "nullable": true
          },
          "batchId": {
            "type": "string",
            "format": "uuid"
          },
          "periodStart": {
            "type": "string",
            "format": "date-time"
          },
          "periodEnd": {
            "type": "string",
            "format": "date-time"
          },
          "resolution": {
            "type": "string"
          },
          "unit": {
            "type": "string"
          },
          "executionTimeStart": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "executionTimeEnd": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "executionState": {
            "$ref": "#/components/schemas/BatchState"
          },
          "areSettlementReportsCreated": {
            "type": "boolean"
          },
          "gridAreaCodes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "processType": {
            "$ref": "#/components/schemas/ProcessType"
          },
          "createdByUserId": {
            "type": "string",
            "format": "uuid"
          }
        }
      },
      "BatchRequestDto": {
        "type": "object",
        "description": "An immutable request to create a batch.",
        "additionalProperties": false,
        "properties": {
          "processType": {
            "$ref": "#/components/schemas/ProcessType"
          },
          "gridAreaCodes": {
            "type": "array",
            "items": {
              "type": "string"
            }
          },
          "startDate": {
            "type": "string",
            "format": "date-time"
          },
          "endDate": {
            "type": "string",
            "format": "date-time"
          }
        }
      },
      "BatchState": {
        "type": "string",
        "enum": [
          "Pending",
          "Executing",
          "Completed",
          "Failed"
        ]
      },
      "ProcessType": {
        "type": "string",
        "description": "Defines the wholesale process type",
        "enum": [
          "BalanceFixing",
          "Aggregation",
          "WholesaleFixing",
          "FirstCorrectionSettlement",
          "SecondCorrectionSettlement",
          "ThirdCorrectionSettlement"
        ]
      }
    },
    "securitySchemes": {
      "Bearer": {
        "type": "http",
        "description": "JWT Authorization header using the Bearer scheme. Example: \"Authorization: Bearer {token}\"",
        "scheme": "bearer"
      }
    }
  },
  "security": [
    {
      "Bearer": [
        "Bearer"
      ]
    }
  ]
}