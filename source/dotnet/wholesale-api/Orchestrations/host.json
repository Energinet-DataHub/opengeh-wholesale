{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      // To be able to specify Task Hub Name we must disable multitenancy in the SQL database
      // See https://microsoft.github.io/durabletask-mssql/#/multitenancy
      "hubName": "%OrchestrationsTaskHubName%",
      // See configuration possibilities here: https://microsoft.github.io/durabletask-mssql/#/quickstart?id=hostjson-configuration
      "storageProvider": {
        "type": "mssql",
        "connectionStringName": "SQLAZURECONNSTR"
      }
    }
  },
  "logging": {
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      },
      "enableLiveMetricsFilters": true
    }
  }
}